n=int(input())
if(n>0 and n<20):
    if(n&1==0):
        print('WEIRD NUMBER')
    else:
        print('NORMAL NUMBER')
elif(n>=20 and n<30):
    print('NORMAL NUMBER')
elif(n>=30 and n&1==1):
    print('NORMAL NUMBER')
elif(n>=30 and n&1==0):
    print('WEIRD NUMBER')
    
